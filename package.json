{
  "name": "possessions",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "beautiful-react-hooks": "^0.31.0",
    "bgio-postgres": "^1.0.13",
    "boardgame.io": "^0.43.3",
    "esm": "^3.2.25",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.20",
    "pg-native": "^3.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-tracked": "^1.6.5",
    "styled-components": "^5.2.1",
    "typeface-domine": "^1.1.13",
    "typeface-staatliches": "^1.1.13",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "node -r esm src/server.js",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve_dev": "node -r esm src/devServer.js",
    "lint": "node_modules/.bin/eslint --ext js src"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope": 0,
      "import/prefer-default-export": 0,
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never"
        }
      ],
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "no-use-before-define": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^7.30.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.25.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prop-types": "^15.7.2",
    "react-is": "^17.0.2",
    "scheduler": "^0.20.2"
  }
}
